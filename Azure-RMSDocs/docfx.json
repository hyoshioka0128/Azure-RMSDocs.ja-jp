{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "dest": "Azure-RMSDocs",
    "template": "docs.html",
    "globalMetadata": {
      "feedback_system": "GitHub",
      "uhfHeaderId": "azure",
      "feedback_github_repo": "MicrosoftDocs/azure-rmsdocs.ja-jp",
      "feedback_product_url": "https://office365.uservoice.com/forums/928576-microsoft-information-protection-mip",
      "layout": "Conceptual",
      "breadcrumb_path": "/azure/information-protection/bread/toc.json",
      "searchScope": [
        "Information Protection"
      ],
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ]
    },
    "markdownEngineName": "markdig"
  }
}
